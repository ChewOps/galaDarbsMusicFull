{{outlet}}
<h2>
  All songs:
</h2>
  <ul>
    {{#each model as |song|}}
      <li>
	    <p>
        <b>
          {{song.artist}} - {{song.title}} {{#link-to 'edit' song.id}}EDIT{{/link-to}}<br>
        </b>
        Released: {{song.year}} <br>
        Genre: {{song.genre}} <br>
        Lyrics:<br> {{song.lyrics}}<br><br>
		</p>
      </li>
    {{/each}}
  </ul>

